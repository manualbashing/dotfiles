{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "shell",
          "style": "diamond",
          "leading_diamond": "",
          "foreground": "#ffffff",
          "background": "#0077c2",
          "template": " {{ .Name }} ({{ .Version }}) ",
          "properties": {
            "mapped_shell_names": {
              "pwsh": "PowerShell"
            }
          }
        },
        {
          "type": "git",
          "style": "plain",
          "leading_diamond": "",
          "foreground": "#193549",
          "background": "#ffeb3b",
          "background_templates": [
            " {{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }} ",
            " {{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }} ",
            " {{ if gt .Ahead 0 }}#B388FF{{ end }} ",
            " {{ if gt .Behind 0 }}#B388FB{{ end }} "
          ],
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uF044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uF046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uF692 {{ .StashCount }}{{ end }} ",
          "properties": {
            "fetch_status": true,
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          }
        },
        {
          "background": "#585c5f",
          "foreground": "#10f596",
          "properties": {
            "folder_icon": "\uf115",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "\uf7db",
            "style": "agnoster"
          },
          "style": "diamond",
          "trailing_diamond": "",
          "template": " {{ .Path }} ",
          "type": "path"
        }
      ],
      "type": "prompt",
      "vertical_offset": 1
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#E06C75",
          "template": "\u276F"
        }
      ],
      "type": "prompt",
      "vertical_offset": 1
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#E06C75",
    "template": "\u276F "
  },
  "final_space": true,
  "version": 2
}
